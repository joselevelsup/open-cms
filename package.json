  },
  "keywords": [
    "react",
    "cms",
    "content management system"
  ],
  "files": [
    "build",
    "README.md",
    "src"
  ],
  "author": "joselevelsup",
  "license": "MIT",
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/hookrouter": "^2.2.2",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@types/yup": "^0.29.3",
    "axios": "^0.19.2",
    "copyfiles": "^2.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "formik": "^2.1.4",
    "hookrouter": "^1.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "parcel": "^1.12.4",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-gate-duo": "github:joselevelsup/react-gate-duo#master",
    "react-icons": "^3.10.0",
    "sass": "^1.26.5",
    "styled-components": "^5.1.1",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.3",
    "yup": "^0.29.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": "/tests/.*\\.(tsx)$",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {}
